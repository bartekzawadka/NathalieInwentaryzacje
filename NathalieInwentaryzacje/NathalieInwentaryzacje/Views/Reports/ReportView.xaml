<controls1:MetroWindow x:Class="NathalieInwentaryzacje.Views.Reports.ReportView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NathalieInwentaryzacje.Views.Reports"
        xmlns:controls="clr-namespace:NathalieInwentaryzacje.Views.Controls"
        xmlns:controls1="http://metro.mahapps.com/winfx/xaml/controls"
        mc:Ignorable="d"
        Title="ReportView" Height="300" Width="300" WindowStartupLocation="CenterScreen" WindowState="Maximized" Style="{StaticResource NWindow}">
    <Grid>
        <controls:ReportPreview Data="{Binding Data, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"></controls:ReportPreview>
    </Grid>
</controls1:MetroWindow>

